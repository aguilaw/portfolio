section#code.section-right
  .row.row--no-gutter  
    .info-wrap.col.col--4-of-12.col--push-8-of-12
      .bar-wrap
        .primary-bar
        .secondary-bar
      h1.title code
      .icon-wrap.col.col--1-of-12
        .icon-design.icon
        .icon-code.icon 
        .icon-hedron.icon
      p.blurb
  .thumb-wrap
    ul.thumb-container
      |{% for project in site.data.settings.code-projects %}
      include templates/thumbnails.jade
      |{% endfor %}
